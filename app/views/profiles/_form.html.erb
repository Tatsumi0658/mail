<% if @profile.errors.any? %>
  <% @profile.errors.full_messages do |msg| %>
    <%= msg %>
  <% end %>
<% end %>
<%= form_with(model:@profile, local:true) do |f| %>
  <%= f.label:username %>
  <%= f.text_field:username %>

  <%= f.label:image %>
  <%= f.file_field:image %>

  <%= f.label:content %>
  <%= f.text_area:content %>

  <%= f.submit "登録する" %>
<% end %>
